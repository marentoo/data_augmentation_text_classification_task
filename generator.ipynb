        "  response = openai.ChatCompletion.create(\n",
        "      model=model,\n",
        "      messages=[{\"role\": \"system\", \"content\": \"You are a helpful assistant. Output sentences separated by newline in reply to <prompt>. Sentences should vary in type, slang, length, structure, tone and style, sentences such as comments, responses, opinions, and facts. Itâ€™s not necessary to often use the emotionâ€™s name in every sentence. Do not number output or use bullet point for the output\"\n",
        "      },\n",
        "                {\"role\": \"user\", \"content\": prompt}],\n",
        "      max_tokens=max_tokens ##, temperature = 0.7 ##<---- when adding temperature parameter\n",
        "  )\n",
        "\n",
        "  end_time = time.time()\n",
        "  time_taken = end_time - start_time\n",
        "  tokens_used = response['usage']['total_tokens']    # Count the tokens used in this response\n",
        "  with open(f\"api_performance_log_{generation_id}.txt\", \"a\") as log_file:    # Log every API call time\n",
        "    log_file.write(f\"{time.strftime('%Y-%m-%d %H:%M:%S')},{time_taken:.2f},{tokens_used}\\n\")\n",
        "  if time_taken > TIME_THRESHOLD: print(f\"WARNING: API call took {time_taken:.2f} seconds, exceeding the threshold.\")\n",
        "\n",
        "  total_api_calls += 1; request_count +=1\n",
        "\n",
        "  if total_api_calls % 60 == 0: print(f\"Total API calls made so far: {total_api_calls}\") # If you've made a multiple of 60 calls, print the count\n",
        "\n",
        "  generation = response.choices[0].message[\"content\"].strip().split('\\n') ##split on newline as in system content message\n",
        "\n",
        "  generations = [p.strip() for p in generation if p.strip() != \"\"]\n",
        "  generations_list.extend(generations)\n",
        "\n",
        "  # print(f\"Generated output: {generations_list}\")  # Print the generated output\n",
        "\n",
        "  return generations_list\n",
        "\n",
        "##------------------------------------------------------------------\n",
        "## retry strategy\n",
        "MAX_RETRIES = 5\n",
        "\n",
        "@retry(\n",
        "    wait=wait_exponential(multiplier=1,min=4, max=30),\n",
        "    stop=stop_after_attempt(MAX_RETRIES),\n",
        "    retry=retry_if_exception_type(openai.APIError)\n",
        ")\n",
        "\n",
        "def safe_generate(*args, **kwargs):\n",
        "  return generate(*args, **kwargs)"
      ]
    },
