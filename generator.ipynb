        "def translate_deepl(sentence, target_language): ## if needed can be specified also source language -- model detects automaticly-- but if neeeded can be added!\n",
        "  api_key = \"***"\n",
        "  url = 'https://api.deepl.com/v2/translate'\n",
        "  headers = {\n",
        "    'Authorization': f'DeepL-Auth-Key {api_key}'\n",
        "  }\n",
        "  data = {\n",
        "    'text': sentence,\n",
        "    'target_lang': target_language\n",
        "  }\n",
        "  # Using a try-except block to catch potential request exceptions\n",
        "  try:\n",
        "      response = requests.post(url, headers=headers, data=data)\n",
        "      if response.status_code == 200:\n",
        "          return response.json()['translations'][0]['text']\n",
        "      else:\n",
        "          # Printing the error response from DeepL to get more information\n",
        "          print(f\"Error code: {response.status_code}\")\n",
        "          print(f\"Error response: {response.text}\")\n",
        "          return \"Error\"\n",
        "  except requests.exceptions.RequestException as e:\n",
        "      # Catching and printing any request exceptions\n",
        "      print(f\"Request failed: {e}\")\n",
        "      return \"Error\""
      ]
