      "source": [
        "##Save logs\n",
        "log = f\"Dataset: GoEmotion - Augmentation of unrepresented classes.\\nAfter preprocessing data frame includes:\\nTotal number of examples: {len(full_df)},\\nTotal numbers of examples for each emotion is: grief: {len(grief_df)}, pride: {len(pride_df)}, relief: {len(relief_df)}, nervousness: {len(nervousness_df)}, embarrassment: {len(embarrassment_df)}\\n\"\n",
        "with open(f\"log_{generation_id}.txt\", \"w\") as log_file:\n",
        "  log_file.write(log)\n",
        "\n",
        "with open(f\"api_performance_log_{generation_id}.txt\", \"w\") as log_file:\n",
        "    log_file.write(\"Timestamp,Time Taken (s),Tokens Used\\n\")"
      ]
