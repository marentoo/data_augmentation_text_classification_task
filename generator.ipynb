        "## Function - sequentional call\n",
        "request_count = 0; total_api_calls = 0; TIME_THRESHOLD = 10.0; MAX_RPM = 60 ## adjust accordingly to request rate limits of openAI API\n",
        "\n",
        "def translate_openai(sentence, model, max_tokens, crafted_prompt, target_language):\n",
        "  global request_count, total_api_calls\n",
        "\n",
        "  if request_count >= MAX_RPM:\n",
        "    print(f\"Reached {request_count} requests. Waiting for the next 60 seconds to continue...\")\n",
        "    time.sleep(5); request_count = 0\n",
        "\n",
        "  prompt = f'{crafted_prompt} {sentence}'\n",
        "  start_time = time.time()\n",
        "\n",
        "  response = openai.ChatCompletion.create(\n",
        "      model=model,\n",
        "      messages=[{\"role\": \"system\", \"content\": f\"You are a Translator. You can recognize source language of the sentence and translate to {target_language}! Output only the translation, without any additional context or introduction! Do not translate token: [NAME]\"},\n",
        "                {\"role\": \"user\", \"content\": prompt}],\n",
        "      max_tokens=max_tokens ##, temperature = 0.7 ##<---- when adding temperature parameter\n",
        "  )\n",
        "\n",
        "  end_time = time.time()\n",
        "  time_taken = end_time - start_time\n",
        "  tokens_used = response['usage']['total_tokens']    # Count the tokens used in this response\n",
        "  with open(f\"api_performance_log_{generation_id}.txt\", \"a\") as log_file:    # Log every API call time\n",
        "    log_file.write(f\"{time.strftime('%Y-%m-%d %H:%M:%S')},{time_taken:.2f},{tokens_used}\\n\")\n",
        "\n",
        "  if time_taken > TIME_THRESHOLD: print(f\"WARNING: API call took {time_taken:.2f} seconds, exceeding the threshold.\")\n",
        "\n",
        "  total_api_calls += 1; request_count +=1\n",
        "  if total_api_calls % 60 == 0: print(f\"Total API calls made so far: {total_api_calls}\") # If you've made a multiple of 60 calls, print the count\n",
        "\n",
        "  translated = response.choices[0].message[\"content\"].strip()\n",
        "  return translated\n",
        "\n",
