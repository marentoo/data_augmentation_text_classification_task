      "source": [
        "## I sub-method for backtranslation - avaible models from HuggingFace\n",
        "\n",
        "def translate(text, model_name):\n",
        "  placeholder = \"[12345]\"     ## chose placeholder - special unique token that won't be tranlsated with model / depending on language and model\n",
        "  replaced_text = text.replace(\"[NAME]\", placeholder)\n",
        "\n",
        "  tokenizer = MarianTokenizer.from_pretrained(model_name)\n",
        "  model = MarianMTModel.from_pretrained(model_name)\n",
        "\n",
        "  translated = model.generate(**tokenizer(replaced_text, return_tensors='pt', padding=True, truncation = True))\n",
        "  translated_text = tokenizer.decode(translated[0], skip_special_tokens=True)\n",
        "  re_replace_text = translated_text.replace(placeholder, \"[NAME]\")\n",
        "\n",
        "  return re_replace_text"
      ]
    },
