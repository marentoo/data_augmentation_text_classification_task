      "source": [
        "##Save logs\n",
        "log = f\"Dataset: GoEmotion - Augmentation of unrepresented classes.\\nAfter preprocessing data frame includes:\\nTotal number of examples: {len(full_df)},\\nTotal numbers of examples for each emotion is: grief: {len(grief_df)}, pride: {len(pride_df)}, relief: {len(relief_df)}, nervousness: {len(nervousness_df)}, embarrassment: {len(embarrassment_df)}\\n\"\n",
        "with open(f\"log_{generation_id}.txt\", \"w\") as log_file:\n",
        "  log_file.write(log)\n",
        "\n",
        "with open(f\"api_performance_log_{generation_id}.txt\", \"w\") as log_file:\n",
        "    log_file.write(\"Timestamp,Time Taken (s),Tokens Used\\n\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "dd4zF5hNx0uH"
      },
      "outputs": [],
      "source": [
        "## Function for generating paraphrases (method 1)\n",
        "\n",
        "request_count = 0; total_api_calls = 0; TIME_THRESHOLD = 10.0; MAX_RPM = 60 ## adjust accordingly to request rate limits of openAI API\n",
        "\n",
        "def generate_paraphrase(input_text, number_augmented_per_example, model, max_tokens, crafted_prompt):\n",
        "  paraphrases_list = []; global request_count, total_api_calls\n",
        "\n",
        "  for _ in range(number_augmented_per_example):\n",
        "    if request_count >= MAX_RPM:\n",
        "      print(f\"Reached {request_count} requests. Waiting for the next 60 seconds to continue...\")\n",
        "      time.sleep(60); request_count = 0\n",
        "\n",
        "    prompt = f'{crafted_prompt} {input_text}'\n",
        "    start_time = time.time()\n",
        "\n",
        "    response = openai.ChatCompletion.create(\n",
        "        model=model,\n",
        "        messages=[{\"role\": \"system\", \"content\": \"You are a helpful assistant. Output sentences separated by newline in reply to <prompt>. Do not number output or use bullet point for the output\"},\n",
        "                  {\"role\": \"user\", \"content\": prompt}],\n",
        "        max_tokens=max_tokens ##, temperature = 0.7 ##<---- when adding temperature parameter\n",
        "    )\n",
        "\n",
        "    end_time = time.time()\n",
        "    time_taken = end_time - start_time\n",
        "    tokens_used = response['usage']['total_tokens']    # Count the tokens used in this response\n",
        "    with open(f\"api_performance_log_{generation_id}.txt\", \"a\") as log_file:    # Log every API call time\n",
        "      log_file.write(f\"{time.strftime('%Y-%m-%d %H:%M:%S')},{time_taken:.2f},{tokens_used}\\n\")\n",
        "\n",
        "    if time_taken > TIME_THRESHOLD: print(f\"WARNING: API call took {time_taken:.2f} seconds, exceeding the threshold.\")\n",
        "\n",
        "    total_api_calls += 1; request_count +=1\n",
        "    if total_api_calls % 60 == 0: print(f\"Total API calls made so far: {total_api_calls}\") # If you've made a multiple of 60 calls, print the count\n",
        "\n",
        "    paraphrase = response.choices[0].message[\"content\"].strip().split('\\n') ##split on newline as in system content message\n",
        "\n",
        "    paraphrases = [p.strip() for p in paraphrase if p.strip() != \"\"]\n",
        "    # paraphrases = [re.sub(r\"^\\d+\\.\\s*\", \"\", p.strip()) for p in paraphrase if p.strip() != \"\"] ## get rid of numbered sentences\n",
        "    paraphrases_list.extend(paraphrases)\n",
        "\n",
        "  return paraphrases_list"
      ]
    },
